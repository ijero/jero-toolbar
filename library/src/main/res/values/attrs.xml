<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="JeroToolbar">
        <attr name="jt_title" format="string" />
        <attr name="jt_leftText" format="string" />
        <attr name="jt_rightText" format="string" />

        <attr name="jt_leftIcon" format="reference" />
        <attr name="jt_rightIcon" format="reference" />

        <attr name="jt_leftTextSize" format="dimension" />
        <attr name="jt_titleTextSize" format="dimension" />
        <attr name="jt_rightTextSize" format="dimension" />

        <attr name="jt_leftTextColor" format="color" />
        <attr name="jt_titleTextColor" format="color" />
        <attr name="jt_rightTextColor" format="color" />

        <attr name="jt_showTitle" format="boolean" />
        <attr name="jt_showLeftText" format="boolean" />
        <attr name="jt_showLeftIcon" format="boolean" />
        <attr name="jt_showRightText" format="boolean" />
        <attr name="jt_showRightIcon" format="boolean" />

        <attr name="jt_titleEnable" format="boolean" />
        <attr name="jt_leftIconEnable" format="boolean" />
        <attr name="jt_leftTextEnable" format="boolean" />
        <attr name="jt_rightIconEnable" format="boolean" />
        <attr name="jt_rightTextEnable" format="boolean" />

        <attr name="android:background" />
    </declare-styleable>
</resources>